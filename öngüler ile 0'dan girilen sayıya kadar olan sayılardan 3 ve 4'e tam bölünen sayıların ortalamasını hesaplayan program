package com.company;


import java.util.Scanner;


public class Main {
    public static void main(String[]args) {
        int k,toplam=0,a=0;
        Scanner scanner = new Scanner(System.in);
        System.out.println("SayÄ± giriniz:");
        k= scanner.nextInt();
        for (int i=0;i<=k;i++){
            if (i%3==0 && i%4==0){
                a=a+1;
                toplam=toplam+i;

            }
        }
        int ortalama=toplam/a;
        System.out.println(ortalama);
    }

}
