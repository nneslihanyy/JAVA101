package com.company;
import java.util.Scanner;
public class Main {
    public static void main(String[]args){
       Scanner scanner= new Scanner(System.in);
      double armut,elma,domates,muz,patlican,odenecektutar;
        System.out.println("Armut kaç kilo:");
        armut = scanner.nextDouble();
        System.out.println("Elma kaç kilo:");
        elma = scanner.nextDouble();
        System.out.println("Domates kaç kilo:");
        domates = scanner.nextDouble();
        System.out.println("Muz kaç kilo:");
        muz = scanner.nextDouble();
        System.out.println("Patlıcan kaç kilo:");
        patlican= scanner.nextDouble();
        odenecektutar=(armut*2.14)+(elma*3.67)+(domates*1.11)+(muz*0.95)+(patlican*5);
        System.out.println("Ödeyeceğiniz tutar:"+odenecektutar);
        }
        }
